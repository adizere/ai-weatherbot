<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
  <class name="model.Flag" table="Flag">
	  <id name="key" type="long" column="key" >
	   	<generator class="increment"/>
	  </id>
	  
	  <property name="value">
		 <column name="value" />
	  </property>
	  
	  <set name="associatedWords" table="WordFlag" cascade="all">
            <key column="flagKey"/>
            <many-to-many class="Word"/>
      </set>
	  
   </class>

</hibernate-mapping>