<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
  <class name="model.Domain" table="Domain">
	  <id name="id" type="long" column="id" >
	   	<generator class="native"/>
	  </id>
	  
	  <property name="lowerBound" type="double">
		 <column name="lowerBound" />
	  </property>
	  
	  <property name="upperBound" type="double">
		 <column name="upperBound" />
	  </property>
	  
	   <bag name="variables" inverse="true" cascade="all,delete-orphan">
      	<key column="domainId"/>
      	<one-to-many class="model.Variable"/>
      </bag>
	  
   </class>

</hibernate-mapping>