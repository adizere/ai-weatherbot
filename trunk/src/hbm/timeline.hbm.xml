<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping>
  <class name="model.Timeline" table="Timeline">
	  <id name="id" type="long" column="id" >
	   	<generator class="increment"/>
	  </id>
	  
	  <property name="begin" type="calendar">
		 <column name="begin"/>
	  </property>
	  
	  <property name="end" type="calendar">
		 <column name="end"/>
	  </property>
	  
   </class>

</hibernate-mapping>